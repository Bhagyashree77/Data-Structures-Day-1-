#include<stdio.h>
#include<conio.h>
main()
{
    char ch;
    scanf("%c",&ch);
    if ((ch>='a'&&ch<='z')||(ch>='A'&&ch<='Z'))
    {
        printf("Alphabet\n",ch);
    }
    else if (ch>='0'&&ch<='9')
    {
        printf("Digit\n",ch);
    }
    else
    {
        printf("Special symbol\n",ch);
    }
}
